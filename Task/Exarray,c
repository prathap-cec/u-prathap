#include <stdio.h>

int main() {
    int arr[7] = {10,20,30,40,50,60,70}; 
    int size = 5;
    int pos = 3;
    int value = 48;

    // Shift  element to the right 
    for(int i = size; i > pos; i--){
        arr[i] = arr[i-1];

    }

    arr[pos] = value;
    size++;

    printf("Array after insertion:\n");
     for(int i = 0; i< size; i++) {
        printf("%d ", arr[i]);
     }

    return 0;
}